{
  "name": "@farajabien/slug-store-server",
  "version": "2.0.0",
  "description": "Server-side state persistence with multiple backends for Slug Store",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js",
      "require": "./dist/adapters/index.cjs"
    },
    "./frameworks": {
      "types": "./dist/frameworks/index.d.ts",
      "import": "./dist/frameworks/index.js",
      "require": "./dist/frameworks/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "server-side",
    "state-management",
    "caching",
    "persistence",
    "redis",
    "slug-store",
    "nextjs",
    "remix",
    "astro",
    "typescript",
    "node"
  ],
  "author": "Faraja Bien <hello@fbien.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/farajabien/slug-store.git"
  },
  "bugs": {
    "url": "https://github.com/farajabien/slug-store/issues"
  },
  "homepage": "https://slugstore.fbien.com",
  "dependencies": {
    "@farajabien/slug-store-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "eslint": "^9.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "redis": "^4.0.0",
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "redis": { "optional": true },
    "ioredis": { "optional": true }
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
} 